
def repeat(x):
    _size=len(x)
    repeated=[]
    for i in range(_size):
        k=i+1
        for j in range(k, _size):
                if x[i] == x[j] and x[i] not in repeated:
                    repeated.append(x[i])
        return repeated

        
list1=[1,2,1,4,5]
print(repeat(list1))

output
[1]
